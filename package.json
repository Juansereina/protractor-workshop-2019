    "jasmine-spec-reporter": "^4.2.1",
    "protractor": "^5.4.2"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.18",
    "typescript": "^3.2.2"
  }
